    "typescript": "^3.0.3",
    "watch": "^1.0.2",
    "webpack": "^4.20.1",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^3.1.1"
  },
  "author": "",
  "license": "Apache-2.0",
  "optionalDependencies": {
    "appformer-js": "^1.0.0"
  },
