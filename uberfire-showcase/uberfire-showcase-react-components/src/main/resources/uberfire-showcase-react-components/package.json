  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:ts-exporter": "npx webpack && npm unpublish -f -reg http://localhost:4873 && yarn publish --new-version ${npm_package_version} --registry http://localhost:4873",
    "reload": "bash -c 'npx webpack && (find ${0} | grep public/${npm_package_name}.js | xargs -I {} cp -v ${npm_package_main} {})'",
    "reload:auto": "bash -c 'npx watch \"yarn run reload -- ${0}\" src'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^0.1.19",
